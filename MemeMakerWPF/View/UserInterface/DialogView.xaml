<UserControl x:Class="MemeMakerWPF.View.UserInterface.DialogView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:material="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:MemeMakerWPF.View.UserInterface"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             KeyDown="UserControl_KeyDown">
    <material:DialogHost x:Name="DialogObject" Identifier="RootDialog" material:TransitionAssist.DisableTransitions="True"
                         BorderBrush="{StaticResource MaterialDesignDivider}">
        <ContentControl x:Name="ContentObject"/>
    </material:DialogHost>
</UserControl>
